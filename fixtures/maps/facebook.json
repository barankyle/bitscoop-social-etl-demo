{
  "version": "1.0",
  "name": "Facebook",
  "url": "https://graph.facebook.com/v2.11",
  "auth": {
	"type": "oauth2",
	"redirect_url": "https://facebook.com",
	"authorization_url": "https://www.facebook.com/dialog/oauth",
	"access_token": "https://graph.facebook.com/oauth/access_token",
	"signature": "parameter",
	"auth_key": "***INSERT AUTH KEY HERE***",
	"auth_secret": "***INSERT AUTH SECRET HERE***"
  },
  "endpoints": {
	"UserProfile": {
	  "GET": {
		"method": "GET",
		"route": {
		  "data": "",
		  "path": "/me"
		}
	  }
	},
	"UserFeed": {
	  "GET": {
		"method": "GET",
		"scopes": [
		  "user_posts"
		],
		"route": {
		  "data": "",
		  "path": "/me/feed"
		},
		"parameters": {
		  "with": {
			"value": "{{ parameters.with }}"
		  }
		}
	  }
	},
	"UserBusinessActivities": {
	  "GET": {
		"method": "GET",
		"route": {
		  "data": "",
		  "path": "/me/business_activities"
		},
		"parameters": {
		  "business": {
			"value": "{{ parameters.business }}"
		  },
		  "offset": {
			"value": "{{ parameters.offset }}"
		  },
		  "until": {
			"value": "{{ parameters.until }}"
		  },
		  "since": {
			"value": "{{ parameters.since }}"
		  },
		  "before": {
			"value": "{{ parameters.before }}"
		  },
		  "after": {
			"value": "{{ parameters.after }}"
		  },
		  "limit": {
			"value": "{{ parameters.limit }}"
		  }
		}
	  }
	},
	"UserOutbox": {
	  "GET": {
		"method": "GET",
		"route": {
		  "data": "",
		  "path": "/me/outbox"
		}
	  }
	},
	"UserConversations": {
	  "GET": {
		"method": "GET",
		"route": {
		  "data": "data",
		  "path": "/me/conversations"
		}
	  }
	}
  }
}